<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\..\FrameXML.old\UI.xsd">
  <Script file="LootFrame.lua" />
  <Button name="ItemEntryDetailsTemplate" virtual="true">
    <Size x="380" y="52"/>
    <Anchors>
      <Anchor point="LEFT" />
    </Anchors>
    <Backdrop bgFile="Interface\FrameGeneral\UI-Background-Marble"
              edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
      <BackgroundInsets>
        <AbsInset left="5" right="5" top="5" bottom="5"/>
      </BackgroundInsets>
      <TileSize>
        <AbsValue val="256"/>
      </TileSize>
      <EdgeSize>
        <AbsValue val="16"/>
      </EdgeSize>
    </Backdrop>
    <Layers>
      <Layer>
        <Texture parentKey="ItemTexture">
          <Anchors>
            <Anchor point="LEFT" relativePoint="LEFT">
              <Offset x="0" />
            </Anchor>
          </Anchors>
          <Size x="48" y="48" />
        </Texture>
        <FontString name="$parentTestString" text="this is some text" inherits="GameFontNormal" parentKey="ItemName" justifyH="LEFT">
          <Size x="200" y="20" />
          <Anchors>
            <Anchor point="LEFT" relativePoint="LEFT">
              <Offset x="55" y="0"/>
            </Anchor>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
    <Frames>
      <Button inherits="LootRollButtonTemplate" parentKey="needButton" id="1">
        <Anchors>
          <Anchor point="TOPRIGHT">
            <Offset>
              <AbsDimension x="-70" y="-14"/>
            </Offset>
          </Anchor>
        </Anchors>
        <Scripts>
          <OnLoad>
            self.tooltipText=NEED;
            self.newbieText=NEED_NEWBIE;
          </OnLoad>
        </Scripts>
        <NormalTexture file="Interface\Buttons\UI-GroupLoot-Dice-Up"/>
        <HighlightTexture file="Interface\Buttons\UI-GroupLoot-Dice-Highlight" alphaMode="ADD"/>
        <PushedTexture file="Interface\Buttons\UI-GroupLoot-Dice-Down"/>
      </Button>
      <Button  inherits="LootRollButtonTemplate" parentKey="greedButton" id="2">
        <Anchors>
          <Anchor point="TOPRIGHT">
            <Offset>
              <AbsDimension x="-38" y="-14"/>
            </Offset>
          </Anchor>
        </Anchors>
        <Scripts>
          <OnLoad>
            self.tooltipText=GREED;
            self.newbieText=GREED_NEWBIE;
          </OnLoad>
        </Scripts>
        <NormalTexture file="Interface\Buttons\UI-GroupLoot-Coin-Up"/>
        <HighlightTexture file="Interface\Buttons\UI-GroupLoot-Coin-Highlight" alphaMode="ADD"/>
        <PushedTexture file="Interface\Buttons\UI-GroupLoot-Coin-Down"/>
      </Button>
      <Button inherits="LootRollButtonTemplate" parentKey="passButton" id="0">
        <Anchors>
          <Anchor point="TOPRIGHT">
            <Offset>
              <AbsDimension x="-5" y="-10"/>
            </Offset>
          </Anchor>
        </Anchors>
        <Scripts>
          <OnLoad>
            self.tooltipText="Pass";
            self.newbieText="Pass";
          </OnLoad>
        </Scripts>
        <NormalTexture file="Interface\Buttons\UI-GroupLoot-Pass-Up"/>
        <HighlightTexture file="Interface\Buttons\UI-GroupLoot-Pass-Highlight" alphaMode="ADD"/>
        <PushedTexture file="Interface\Buttons\UI-GroupLoot-Pass-Down"/>
      </Button>
      
    </Frames>
  </Button>
  <Button name="PlayerRollDetail" virtual="true">
    <Size x="350" y="30" />
    <Anchors>
      <Anchor point="LEFT" />
    </Anchors>
    <Layers>
      <Layer>
        <FontString parentKey="PlayerName" inherits="GameFontNormal">
          <Size x="150" y="20" />
          <Anchors>
            <Anchor point="LEFT" />
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
  </Button>
  <Frame name="LootMLootFrame" parent="UIParent" hidden="true" toplevel="true" enableMouse="true" movable="true">
    <Size>
      <AbsDimension x="400" y="300" />
    </Size>
    <Anchors>
      <Anchor point="CENTER" />
    </Anchors>
    <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background"
          edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
      <BackgroundInsets>
        <AbsInset left="11" right="12" top="12" bottom="11"/>
      </BackgroundInsets>
      <TileSize>
        <AbsValue val="32"/>
      </TileSize>
      <EdgeSize>
        <AbsValue val="32"/>
      </EdgeSize>
    </Backdrop>
    <Frames>
      <Button inherits="UIPanelCloseButton">
        <Anchors>
          <Anchor point="TOPRIGHT">
          </Anchor>
        </Anchors>
      </Button>
      <Frame name="ItemEntryContainer" parentKey="ItemEntryContainer">
        <Size x="200" y="200" />
        <Anchors>
          <Anchor point="TOPLEFT"></Anchor>
        </Anchors>
      </Frame>
    </Frames>
    <Scripts>
      <OnLoad>
        RegisterFrame(self);
      </OnLoad>
    </Scripts>
  </Frame>
</Ui>      
